        function logout()
        {

            sessionStorage.removeItem("fname");
            sessionStorage.removeItem("mname");
            sessionStorage.removeItem("lname");
            sessionStorage.removeItem("type");
            sessionStorage.removeItem("userId");
            sessionStorage.removeItem("image");
            sessionStorage.removeItem("SchoolID");
            //location.href = "/Home/Login?m=logout";
            location.href = "/Home/LoginOut";



        }

        function GoToProfile()
        {
            window.location.href = "/Student/ProfileforStudent?id=" + sessionStorage.getItem("stdID") + "&type=" + sessionStorage.getItem("type");
        }

        function LeaveApply()
        {
            window.location.href = "/Student/StudentLeave?id=" + sessionStorage.getItem("stdID");
        }

        function viewFee() {
            location.href = "/Fees/StudentFeeDashBoard?Id=" + sessionStorage.getItem("stdID");
        }
        function PayFEE() {
            location.href = "/Fees/StudentFeeCalculation?id=" + sessionStorage.getItem("stdID");
        }
        function payfeecalculate()
        {
            var SchoolID = sessionStorage.getItem();
            $.ajax({
                url: "/api/feeapi/feeapifatch",
                type: "Post",
                data: JSON.stringify([SchoolID]),
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    if (data != null) {
                        window.location.href = "" + sessionStorage.getItem("stdID")
                    }
                }

            });
        }
        function TransportallocationCheck()
        {
            var StudentID = sessionStorage.getItem("stdID");
            $.ajax({
                type: "POST",
                url: "/api/TransportApi/CheckTransportFACILITY",
                data: JSON.stringify([StudentID]),
                contentType :"application/json; charset=utf-8" ,
                success: function (success)
                {
                    
                    var val = success.split('&=');
                    var Vehicleid = val[0];
                    var Vehicleno=val[1]
                    var ValidationKey = val[2];
                    Vehicleid + "&=" + Vehicleno + "&=" + ValidationKey
                    if (Vehicleid != "-1" && ValidationKey !="-1" )
                    {
                      
                        window.location.href = "/Student/StudentVehicleCurrentLocation?id=" + Vehicleid + "&Vehicleno=" + Vehicleno+ "&ValidationKey=" + ValidationKey;

                    }
                    else  
                    {
                        alert("Transport FACILITY not available for Student");
                    }

                }

            });



        }
        function ViewTimeTable() {
            var x;
            $.ajax({
                url: "/api/TimeTableApi/GetTimeTableID",
                type: "Post",
                data: JSON.stringify([sessionStorage.getItem("stdID")]),
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    if (data != -1)
                    {
                        window.location.href = "/TimeTable/ViewStudentTimeTable?id=" + data;
                    }
                    else
                    {


                    }
                }
            });
        }

        function checkRoleManagementRigtsSchool()
        {
            $("#li_15").hide();
            $("#li_12").hide();
            $("#li_7").hide();
            $("#li_4").hide();
            $("#li_2").hide();
            $("#li_3").hide();
            $("#li_8").hide();
            $("#li_17").hide();
            $("#li_5").hide();
            $("#li_16").hide();
         
            var SchoolID = sessionStorage.getItem("SchoolID");
            $.ajax({
                type: "POST",
                url: "/api/RolesPermissionApi/checkRoleManagementRigtsSchool",
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify([SchoolID]),
                success: function (data)
                {
                    if (data.list.length > 0)
                    {
                            for (var i = 0; i < data.list.length; i++)
                           {
                             $("#li_" + data.list[i]).show();
                           }


                    }
                }
            });

        }
    </script>
  


