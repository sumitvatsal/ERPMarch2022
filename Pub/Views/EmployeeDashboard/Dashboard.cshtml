            data: JSON.stringify([SchoolID]),           
            contentType: 'application/json; charset=utf-8',
            success: function (data)
            {
                if (data.list.length > 0)
                {
                    for (var i = 0; i < data.list.length; i++)
                    {
                        if (data.list[i] == 6) {
                            $("#ShowEventslist").show();
                           
