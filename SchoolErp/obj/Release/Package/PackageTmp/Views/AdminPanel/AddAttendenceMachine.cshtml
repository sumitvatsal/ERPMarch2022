<script type="text/javascript">

    //callServiceMethodForDDl("", "/api/Default", "getAllSchoolName", "ddlSchoolName", false, "", "", "", "");
    $("#chkstatus").prop('checked', true);
    $("#Hiddenid").val(0);
    $("#btnsubmit").val("Create");
