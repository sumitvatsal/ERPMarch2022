    function SaveDetails(id,sub,emp,status)
    {
        var Items = new Array();
        var assignPeriod={
            ID:id,
            SubjectID: sub,
            TeacherID: emp,
            Status:status
        }
        var Student = {
            ID: id,
            tt: assignPeriod,
            ViewType: "CreateTimeTable"
        }
        Items.push(Student);

        $.ajax({
            url: "/api/TimeTableApi/SaveTiming",
            type: "Post",
            data: JSON.stringify(Items),
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                
                  
