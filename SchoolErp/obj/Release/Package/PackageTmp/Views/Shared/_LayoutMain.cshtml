        function TransportallocationCheck() {
            var EmployeeID = sessionStorage.getItem("EmpId");
            $.ajax({
                type: "POST",
                url: "/api/TransportApi/CheckTransportFACILITYEmployee",
                data: JSON.stringify([EmployeeID]),
                contentType: "application/json; charset=utf-8",
                success: function (success) {

                    var val = success.split('&=');
                    var Vehicleid = val[0];
                    var Vehicleno = val[1]
                    var ValidationKey = val[2];
                  
                    if (Vehicleid != "-1" && ValidationKey != "-1") {
                        

                        window.location.href = "/Transport/EmployeeVehicleCurrentLocation?id=" + Vehicleid + "&Vehicleno=" + Vehicleno + "&ValidationKey=" + ValidationKey;

                    }
                    else
                    {

                        alert("Transport FACILITY not available for Employee");
                    }

                }

            });



        }

