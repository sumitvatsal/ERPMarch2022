        function CheckRolemasterforSchool()
        {
            debugger;
            var SchoolID = sessionStorage.getItem("SchoolID");
            $.ajax({
                url:"/api/RolesPermissionApi/GetAssignedRoleManagementforSchool" ,
                type: "POST",
                data: JSON.stringify([SchoolID]),
                contentType: 'application/json; charset=utf-8',
                success: function (datas) {
                    if (datas.ID != -1 && datas.ID != 0) {
                        $.each(datas.smod, function (index, item) {
                            $("#li_" + item.ModuleID).show();
                            $("#liSubMod_" + item.SubModuleID).show();
                        });

                    }
                }
            });
        }
        function checkRoles() {
            var Id = sessionStorage.getItem("EmpId");
            $.ajax({
                url: "/api/RolesPermissionApi/GetAssignedRoleByEmpID",
                type: "Post",
                data: JSON.stringify([Id]),
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    if (data.ID != -1 && data.ID != 0) {
                        $.each(data.RmodList, function (index, item) {
                            $("#li_" + item.ModuleID).show();
                            $("#liSubMod_" + item.SubmoduleID).show();
                        });
                    }
                }
            });
        }

