                            var a = datas;
                            if (a != "") {
                             
                                setControlValue("lblPwdMsg", a)
                                $("#lblPwdMsg").removeAttr("style");
                                $("#lblPwdMsg").attr("style", "color:green;font-weight:bold");
                                setTimeout(function () {
                                    //$("#lblPwdMsg").attr("style", "display:none");
                                    $("#divSuccessMsg").fadeOut();
                                    setControlValue("txtNewPassword", "");
                                    setControlValue("txtConfirmPassword", "");
                                    setControlValue("txtOldPassword", "");
                                }, 1000)

                            }

                        }

                    });
                }
                else {
                    alert("Please Enter  Confirm Password ");
                }
            }
            else {
                alert("Please Enter  Password ");
            }
        }
        else {
            alert("Please Enter Old Password!");
        }



    }
</script>

